<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="robot_name" />
    <arg name="init_pose" />
    <node args="-param /robot_description -urdf -model $(arg robot_name)" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" />
    <node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher" output="screen" />
</launch>