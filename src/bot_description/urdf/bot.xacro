  <visual>
    <origin rpy="0 0 0" xyz="-0.0 -0.07239 -0.025"/>
    <geometry>
      <mesh filename="package://bot_description/meshes/wheel_2_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin rpy="0 0 0" xyz="-0.0 -0.07239 -0.025"/>
    <geometry>
      <mesh filename="package://bot_description/meshes/wheel_2_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="wheel_1_1">
  <inertial>
    <origin rpy="0 0 0" xyz="-3.618764437576268e-18 -0.010000000000000009 -6.938893903907228e-18"/>
    <mass value="0.03652101459798134"/>
